{
  "name": "mvx-assets-manager-api",
  "version": "1.0.0",
  "description": "MultiversX blockchain API for asset management, based on Buildo Begins",
  "type": "module",
  "main": "build/index.js",
  "scripts": {
    "build": "rimraf build && node ./esbuild.config.js && tsc",
    "start": "node build/index.js",
    "dev": "nodemon --exec node build/index.js",
    "lint": "eslint src/** --fix",
    "prettier": "prettier --write 'src/**/*.{js,ts,json}'",
    "prepare": "npm run build"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@multiversx/sdk-core": "12.17.0",
    "@multiversx/sdk-network-providers": "2.2.0",
    "@multiversx/sdk-transaction-decoder": "1.0.2",
    "@multiversx/sdk-wallet": "4.2.0",
    "express": "^4.18.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "swagger-ui-express": "^5.0.0",
    "express-validator": "^7.0.1",
    "bignumber.js": "9.1.2",
    "axios": "1.6.3",
    "swagger-jsdoc": "^6.2.8",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "20.10.5",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "6.16.0",
    "@typescript-eslint/parser": "6.16.0",
    "esbuild": "0.19.10",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.2",
    "nodemon": "^3.1.0",
    "prettier": "3.1.1",
    "rimraf": "5.0.5",
    "typescript": "5.3.3"
  }
