{
  "name": "mvx-assets-manager-api",
  "version": "1.0.0",
  "description": "MultiversX Assets Manager API",
  "main": "build/index.js",
  "scripts": {
    "start": "node build/index.js",
    "build": "rimraf build && tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@multiversx/sdk-core": "^12.18.1",
    "@multiversx/sdk-wallet": "^4.4.0",
    "adm-zip": "^0.5.10",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.2.0",
    "express-validator": "^7.0.1",
    "keccak": "^3.0.4",
    "ora": "^5.4.1",
    "prompts": "^2.4.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.25",
    "@types/prompts": "^2.4.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
