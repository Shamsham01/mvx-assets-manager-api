# Server Configuration
PORT=3000
NODE_ENV=development

# Network Configuration
TESTNET_API_URL=https://testnet-api.multiversx.com
TESTNET_GATEWAY_URL=https://testnet-gateway.multiversx.com
TESTNET_EXPLORER_URL=https://testnet-explorer.multiversx.com
MAINNET_API_URL=https://api.multiversx.com
MAINNET_GATEWAY_URL=https://gateway.multiversx.com
MAINNET_EXPLORER_URL=https://explorer.multiversx.com

# Security Configuration
API_KEY_HEADER=x-api-key
API_KEYS=your-api-key-1,your-api-key-2
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100

# CORS Configuration
CORS_ENABLED=true
CORS_ORIGIN=*
CORS_METHODS=GET,POST

# PEM File Configuration
PEM_FILE_PATH=./wallet.pem

# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=combined 